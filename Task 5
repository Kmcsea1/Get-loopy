class StarPattern {
    static void createStarPattern() {
        for (int i = 1; i <= 5; i++) {  // Outer loop 
            for (int j = 1; j <= i; j++) {  // Inner loop 
                System.out.print("* ");
            }
            System.out.println();  
        }
    }

    public static void main(String[] args) {
        createStarPattern();  
    }
}
