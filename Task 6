class InvertedStarPattern {
    static void createInvertedStarPattern() {
        for (int i = 5; i >= 1; i--) {  // Outer loop
            for (int j = 1; j <= i; j++) {  // Inner loop
                System.out.print("* ");
            }
            System.out.println();
        }
    }

    public static void main(String[] args) {
        createInvertedStarPattern();  
    }
}
