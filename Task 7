class SquarePattern {
    static void createSquarePattern() {
        for (int i = 1; i <= 5; i++) {  // Outer loop
            for (int j = 1; j <= 5; j++) {  // Inner loop
                System.out.print("* ");
            }
            System.out.println();
        }
    }

    public static void main(String[] args) {
        createSquarePattern();  
    }
}
